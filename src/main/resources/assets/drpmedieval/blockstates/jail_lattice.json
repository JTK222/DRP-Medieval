{
	"multipart": [
		{   "when": { "axis": "x" },
			"apply": {"model": "drpmedieval:block/jail_lattice/jail_lattice"}
		},
		{   "when": { "axis": "z" },
			"apply": {"model": "drpmedieval:block/jail_lattice/jail_lattice", "y" : 90}
		},
		{   "when": { "axis": "x", "top" : "false"},
			"apply": {"model": "drpmedieval:block/jail_lattice/jail_lattice_border_top"}
		},
		{   "when": { "axis": "z", "top" : "false"},
			"apply": {"model": "drpmedieval:block/jail_lattice/jail_lattice_border_top", "y" : 90}
		},

		{   "when": { "axis": "x", "right" : "false"},
			"apply": {"model": "drpmedieval:block/jail_lattice/jail_lattice_border_right"}
		},
		{   "when": { "axis": "z", "right" : "false"},
			"apply": {"model": "drpmedieval:block/jail_lattice/jail_lattice_border_right", "y" : 90}
		},

		{   "when": { "axis": "x", "bottom" : "false"},
			"apply": {"model": "drpmedieval:block/jail_lattice/jail_lattice_border_bottom"}
		},
		{   "when": { "axis": "z", "bottom" : "false"},
			"apply": {"model": "drpmedieval:block/jail_lattice/jail_lattice_border_bottom", "y" : 90}
		},

		{   "when": { "axis": "x", "left" : "false"},
			"apply": {"model": "drpmedieval:block/jail_lattice/jail_lattice_border_left"}
		},
		{   "when": { "axis": "z", "left" : "false"},
			"apply": {"model": "drpmedieval:block/jail_lattice/jail_lattice_border_left", "y" : 90}
		}
	]
}